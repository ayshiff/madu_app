{
    "expo": {
        "name": "madu_test",
        "slug": "madu_test",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#fff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "infoPlist": {
                "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
            }
        },

        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
