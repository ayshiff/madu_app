{
    "main": "./src/App.tsx",
    "scripts": {
        "start": "expo start",
        "format": "npx prettier --write **/*.{js,ts,tsx,json}",
        "lint": "tsc --noEmit && eslint --ext .js,.ts,.tsx ./",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "test": "jest --config ./jest.config.js",
        "eject": "expo eject",
        "coverage": "jest --config ./jest.config.js --env=jsdom --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format && yarn test"
        }
    },
    "dependencies": {
        "@react-native-community/masked-view": "^0.1.10",
        "@react-navigation/bottom-tabs": "^5.6.1",
        "@react-navigation/native": "^5.6.1",
        "@react-navigation/stack": "^5.6.2",
        "expo": "~38.0.8",
        "expo-camera": "^8.3.1",
        "expo-image-picker": "^8.3.0",
        "expo-status-bar": "^1.0.2",
        "ramda": "^0.27.0",
        "react": "~16.11.0",
        "react-dom": "~16.11.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.1.tar.gz",
        "react-native-gesture-handler": "^1.6.1",
        "react-native-maps": "^0.27.1",
        "react-native-screens": "^2.9.0",
        "react-native-web": "~0.11.7",
        "react-navigation": "^4.4.0",
        "react-navigation-hooks": "^1.1.0",
        "react-navigation-stack": "^2.8.2",
        "react-redux": "^7.2.0",
        "redux-observable": "^1.2.0",
        "rxjs": "^6.5.5"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@types/react": "~16.9.41",
        "@types/react-native": "~0.62.13",
        "@types/react-redux": "^7.1.9",
        "prettier": "^2.0.5",
        "typescript": "~3.9.5"
    },
    "private": true
}
